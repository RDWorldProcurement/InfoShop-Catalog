{
  "version": 2,
  "name": "infoshop-omnisupply",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "REACT_APP_BACKEND_URL": "@react_app_backend_url",
    "REACT_APP_ALGOLIA_APP_ID": "@react_app_algolia_app_id",
    "REACT_APP_ALGOLIA_SEARCH_KEY": "@react_app_algolia_search_key"
  }
}
